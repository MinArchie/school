#WAP to accept two lists, point out 'overlapped' elements or if not then the
#seperated elements

while True:
    listA = eval(input("Enter list1: "))
    listB = eval(input("Enter List 2: "))
    len1 = len(listA)
    len2 = len(listB)
    for a in range(len1):
        ele = listA[a]
        if ele in listB:
            print("Overlapped")
            break
    else:
            print("Seperated")
